&iomuxc {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_hog_1>;

	hog {
		pinctrl_hog_1: hoggrp-1 {
			fsl,pins = <
				MX6QDL_PAD_GPIO_0__CCM_CLKO1    	0x130b0 /*WM8960 MCLK*/
//				MX6QDL_PAD_GPIO_6__GPIO1_IO06		0x170b0	/*IR recieve*/
//				MX6QDL_PAD_ENET_TXD1__GPIO1_IO29	0x07048 /*pen IRQ*/
//				MX6QDL_PAD_ENET_TX_EN__GPIO1_IO28	0x00040 /*ext gpio IRQ*/
                MX6QDL_PAD_SD1_DAT1__PWM3_OUT 0x1b0b1 /*0x1b0b1 pwm for motorrl ctrl */         
                MX6QDL_PAD_SD1_CMD__PWM4_OUT 0x1b0b1 /*0x1b0b1 pwm for motorrr ctrl */
            /* motorrl ctrl pin */            
                MX6QDL_PAD_SD3_DAT0__GPIO7_IO04     0x130b0 /* Direction Out PD*/
                MX6QDL_PAD_EIM_A25__GPIO5_IO02      0x10048 /* Capture A */
                MX6QDL_PAD_SD3_DAT3__GPIO7_IO07     0x10048 /* Capture B */
             /* motorrb  */
                MX6QDL_PAD_SD3_DAT1__GPIO7_IO05     0x130b0 /* Direction Out PD*/
                MX6QDL_PAD_SD3_CLK__GPIO7_IO03      0x1b048 /* Capture A */
                MX6QDL_PAD_SD3_CMD__GPIO7_IO02      0x1b048 /* Capture B */         
			>;
		};
	
		pinctrl_gpio_keys: gpio-keysgrp{		
			fsl,pins = <							
				MX6QDL_PAD_GPIO_19__GPIO4_IO05		0x17048 /* Power Button */
				MX6QDL_PAD_KEY_COL1__GPIO4_IO08		0x17048 /* Volume Up */
				MX6QDL_PAD_KEY_ROW1__GPIO4_IO09		0x17048 /* Volume Down */
			>;	
		};

		pinctrl_gpio_powerhold: gpio-powerholdgrp{					
			fsl,pins = <
				MX6QDL_PAD_EIM_BCLK__GPIO6_IO31    	0x17048 /* Power Off:0*/
			>;
		};
		
        pinctrl_gpio_irx: gpio-irxgrp{					
			fsl,pins = <
				MX6QDL_PAD_GPIO_6__GPIO1_IO06		0x170b0	/*IR recieve*/
			>;
		};

		runctrl_pins: runctrlgrp{					
			fsl,pins = <
				MX6QDL_PAD_SD1_DAT0__GPIO1_IO16		0x1b0b1 /*VGA PASAVE OD, PULL  disabled,slow*/
				MX6QDL_PAD_GPIO_17__GPIO7_IO12		0x1b0b1 /*CAMERA RST */
				MX6QDL_PAD_GPIO_18__GPIO7_IO13		0x1b0b1 /*CAMERA PWDN */
			>;
        };

        pinctrl_pwm3_2: pwm3grp-2 {         
            fsl,pins = <                
                MX6QDL_PAD_SD1_DAT1__PWM3_OUT 0x120b1 /*0x1b0b1*/           
            >;      
        };      
        
        pinctrl_pwm4_1: pwm4grp-1 {            
            fsl,pins = <                
                MX6QDL_PAD_SD1_CMD__PWM4_OUT 0x120b1 /*0x1b0b1*/            
            >;      
        };

        pinctrl_gpio_dcmotors:dcmotorsgrp-1{
            fsl,pins = </* motorlb  */
                MX6QDL_PAD_SD3_DAT0__GPIO7_IO04     0x120b1 /* Direction Out*/
                MX6QDL_PAD_EIM_A25__GPIO5_IO02      0x17048 /* Capture A */
                MX6QDL_PAD_SD3_DAT3__GPIO7_IO07     0x17048 /* Capture B */
             /* motorrb  */
                MX6QDL_PAD_SD3_DAT1__GPIO7_IO05     0x120b1 /* Direction Out*/
                MX6QDL_PAD_SD3_CLK__GPIO7_IO03      0x17048 /* Capture A */
                MX6QDL_PAD_SD3_CMD__GPIO7_IO02      0x17048 /* Capture B */         
            >;              
        };
	};
};

&ldb {
	ipu_id = <1>;
	disp_id = <1>;
	ext_ref = <1>;
	mode = "sep1";
	sec_ipu_id = <1>;
	sec_disp_id = <0>;
	status = "disabled";
};

&mipi_csi {
	status = "disabled";
	ipu_id = <0>;
	csi_id = <1>;
	v_channel = <0>;
	lanes = <2>;
};

&pwm1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_pwm1_1>;
	status = "okay";
};

&pwm3 {
    //pinctrl-names = "default"; for sync gpio init; 
    //pinctrl-0 = <&pinctrl_pwm3_2>;
    status = "okay";
};

&pwm4 {
    //pinctrl-names = "default";
    //pinctrl-0 = <&pinctrl_pwm4_1>;
    status = "okay";
};

&ssi2 {
	fsl,mode = "i2s-slave";
	status = "okay";
};

&uart1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart1_2>;
	status = "okay";
	pinctrl_uart1_2: uart1grp-2 {		
           fsl,pins = <			
		MX6QDL_PAD_SD3_DAT7__UART1_TX_DATA 0x1b0b1			
		MX6QDL_PAD_SD3_DAT6__UART1_RX_DATA 0x1b0b1		
		>;	
	};
};

&usbh1 {
	vbus-supply = <&reg_usb_h1_vbus>;
	status = "okay";
};

&usbotg {
	vbus-supply = <&reg_usb_otg_vbus>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usbotg_2>;
	disable-over-current;
	status = "okay";
};

&usdhc2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc2_2>;
	cd-gpios = <&gpio1 4 0>;
	/*wp-gpios = <&gpio2 3 0>;*/
	no-1-8-v;
	keep-power-in-suspend;
	enable-sdio-wakeup;
	status = "okay";
};

&usdhc3 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc3_1>;
	cd-gpios = <&gpio2 0 0>;
	wp-gpios = <&gpio2 1 0>;
	no-1-8-v;
	keep-power-in-suspend;
	enable-sdio-wakeup;
	status = "disabled";
};

&usdhc4 {
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_usdhc4_1>;
        bus-width = <8>;
        non-removable;
        no-1-8-v;
	keep-power-in-suspend;
        status = "okay";
};

&vpu {
	pu-supply = <&pu_dummy>; /* ldo-bypass:use pu_dummy if VDDSOC share with VDDPU */
};
