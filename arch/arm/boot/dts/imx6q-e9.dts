	tsc2007: tsc2007@48 {
		compatible = "ti,tsc2007";
		reg = <0x48>;
		interrupt-parent = <&gpio1>;
		interrupts = <0x0 0x4>;
		gpios = <&gpio1 29 0>;
		ti,x-plate-ohms = <180>;
		ti,poll-period = <10>;
		status = "disabled";
	};

*/	
};

&i2c2 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
